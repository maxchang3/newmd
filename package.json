{
  "name": "newmd",
  "type": "module",
  "version": "0.0.7",
  "packageManager": "pnpm@10.12.4",
  "description": "A CLI tool that creates markdown files with frontmatter using a Zod schema.",
  "author": "Max Chang <maxcfoo@gmail.com>",
  "license": "MIT",
  "homepage": "https://github.com/maxchang3/newmd#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/maxchang3/newmd.git"
  },
  "bugs": "https://github.com/maxchang3/newmd/issues",
  "keywords": [
    "zod",
    "schema",
    "markdown",
    "frontmatter"
  ],
  "sideEffects": false,
  "main": "dist/index.mjs",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "bin": {
    "newmd": "bin/newmd.mjs"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "unbuild",
    "dev": "unbuild --stub",
    "lint": "biome check",
    "lint:fix": "biome check --write .",
    "prepublishOnly": "pnpm run build",
    "release": "bumpp && pnpm publish",
    "start": "esno src/cli.ts",
    "test": "vitest",
    "test:ci": "vitest run",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "ansis": "^4.1.0",
    "clipanion": "4.0.0-rc.4",
    "deepmerge-ts": "^7.1.5",
    "github-slugger": "^2.0.0",
    "js-yaml": "^4.1.0",
    "pathe": "^2.0.3",
    "smol-toml": "^1.4.1",
    "unconfig": "^7.3.2",
    "zod": "^3.25.74",
    "zod-empty": "^1.4.5"
  },
  "devDependencies": {
    "@biomejs/biome": "^2.0.6",
    "@maxchang/biome-config": "^0.1.3",
    "@types/js-yaml": "^4.0.9",
    "@types/node": "^20.19.4",
    "bumpp": "^10.2.0",
    "esno": "^4.8.0",
    "memfs": "^4.17.2",
    "tinyexec": "^1.0.1",
    "typescript": "^5.8.3",
    "unbuild": "^3.5.0",
    "vite-tsconfig-paths": "^5.1.4",
    "vitest": "^3.2.4"
  },
  "pnpm": {
    "onlyBuiltDependencies": [
      "@biomejs/biome",
      "esbuild"
    ]
  }
}

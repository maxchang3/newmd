{
  "name": "newmd",
  "type": "module",
  "version": "0.0.1",
  "packageManager": "pnpm@10.5.2",
  "author": "Max Chang <maxcfoo@gmail.com>",
  "license": "MIT",
  "homepage": "https://github.com/maxchang3/newmd#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/maxchang3/newmd.git"
  },
  "bugs": "https://github.com/maxchang3/newmd/issues",
  "sideEffects": false,
  "main": "dist/index.mjs",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "typesVersions": {
    "*": {
      "*": [
        "./dist/*",
        "./dist/index.d.ts"
      ]
    }
  },
  "bin": {
    "newmd": "bin/newmd.mjs"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "unbuild",
    "dev": "unbuild --stub",
    "lint": "eslint .",
    "lint:fix": "eslint --fix .",
    "prepublishOnly": "pnpm run build",
    "release": "bumpp && pnpm publish",
    "start": "esno src/cli.ts",
    "test": "vitest",
    "typecheck": "tsc --noEmit",
    "prepare": "simple-git-hooks"
  },
  "dependencies": {
    "clipanion": "4.0.0-rc.4",
    "deepmerge": "^4.3.1",
    "github-slugger": "^2.0.0",
    "js-yaml": "^4.1.0",
    "pathe": "^2.0.3",
    "smol-toml": "^1.3.1",
    "unconfig": "^7.3.0",
    "zod": "^3.24.2",
    "zod-empty": "^1.3.4"
  },
  "devDependencies": {
    "@maxchang/eslint-config": "^1.0.16",
    "@types/js-yaml": "^4.0.9",
    "@types/node": "^20.17.23",
    "bumpp": "^10.0.3",
    "eslint": "^9.21.0",
    "esno": "^4.8.0",
    "lint-staged": "^15.4.3",
    "simple-git-hooks": "^2.11.1",
    "tinyexec": "^0.3.2",
    "typescript": "^5.8.2",
    "unbuild": "^3.5.0",
    "vite": "^6.2.1",
    "vite-tsconfig-paths": "^5.1.4",
    "vitest": "^3.0.8"
  },
  "pnpm": {
    "onlyBuiltDependencies": [
      "esbuild",
      "simple-git-hooks"
    ]
  },
  "simple-git-hooks": {
    "pre-commit": "pnpm lint-staged"
  },
  "lint-staged": {
    "*": "eslint --fix"
  }
}

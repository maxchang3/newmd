{
  "name": "newmd",
  "type": "module",
  "version": "0.0.5",
  "packageManager": "pnpm@10.6.1",
  "description": "Create markdown file with frontmatter using Zod schema.",
  "author": "Max Chang <maxcfoo@gmail.com>",
  "license": "MIT",
  "homepage": "https://github.com/maxchang3/newmd#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/maxchang3/newmd.git"
  },
  "bugs": "https://github.com/maxchang3/newmd/issues",
  "sideEffects": false,
  "main": "dist/index.mjs",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "bin": {
    "newmd": "bin/newmd.mjs"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "unbuild",
    "dev": "unbuild --stub",
    "lint": "eslint .",
    "lint:fix": "eslint --fix .",
    "prepublishOnly": "pnpm run build",
    "release": "bumpp && pnpm publish",
    "start": "esno src/cli.ts",
    "test": "vitest",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "ansis": "^3.17.0",
    "clipanion": "4.0.0-rc.4",
    "deepmerge-ts": "^7.1.5",
    "github-slugger": "^2.0.0",
    "js-yaml": "^4.1.0",
    "pathe": "^2.0.3",
    "smol-toml": "^1.3.1",
    "ts-deepmerge": "^7.0.2",
    "unconfig": "^7.3.1",
    "zod": "^3.24.2",
    "zod-empty": "^1.4.1"
  },
  "devDependencies": {
    "@maxchang/eslint-config": "^1.0.16",
    "@types/js-yaml": "^4.0.9",
    "@types/node": "^20.17.24",
    "bumpp": "^10.0.3",
    "eslint": "^9.22.0",
    "esno": "^4.8.0",
    "memfs": "^4.17.0",
    "tinyexec": "^0.3.2",
    "typescript": "^5.8.2",
    "unbuild": "^3.5.0",
    "vite": "^6.2.1",
    "vite-tsconfig-paths": "^5.1.4",
    "vitest": "^3.0.8"
  },
  "pnpm": {
    "onlyBuiltDependencies": [
      "esbuild"
    ]
  }
}
